{
  "name": "class-config-base",
  "version": "0.3.0",
  "description": "The base class of a configuration class for a interfacial class.",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "coveralls": "nyc --reporter=text-lcov npm test | coveralls",
    "web:build": "browserify index.js --standalone ClassConfigBase -o web/class-config-base.js && cd web && uglifyjs class-config-base.js --compress --mangle -o class-config-base.min.js --source-map url=i\"'class-config-base.min.js.map'\"",
    "chrome:install": "npm i --no-save mocha-chrome",
    "chrome:test": "mocha-chrome test/web/browser-test.html",
    "build": "npm run lint && npm run coverage && npm run web:build && node build/webify.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sttk/class-config-base.git"
  },
  "keywords": [
    "class",
    "base",
    "config",
    "private",
    "hidden"
  ],
  "author": "Takayuki Sato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sttk/class-config-base/issues"
  },
  "homepage": "https://github.com/sttk/class-config-base#readme",
  "devDependencies": {
    "browserify": "^16.2.2",
    "chai": "^3.5.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "mocha": "^3.2.0",
    "nyc": "^11.7.2",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {
    "copy-props": "^2.0.0",
    "default-val": "^0.1.5",
    "instance-stringer": "^0.1.0"
  }
}
